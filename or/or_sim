:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f36dfeab240 .scope module, "or_gate_tb" "or_gate_tb" 2 1;
 .timescale 0 0;
v0x5f36dfebd080_0 .var "a", 0 0;
v0x5f36dfebd150_0 .var "b", 0 0;
v0x5f36dfebd220_0 .net "y", 0 0, L_0x5f36dfebd320;  1 drivers
S_0x5f36dfeab3d0 .scope module, "uut" "or_gate" 2 6, 3 1 0, S_0x5f36dfeab240;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "y";
L_0x5f36dfebd320 .functor OR 1, v0x5f36dfebd080_0, v0x5f36dfebd150_0, C4<0>, C4<0>;
v0x5f36dfeab600_0 .net "a", 0 0, v0x5f36dfebd080_0;  1 drivers
v0x5f36dfebcea0_0 .net "b", 0 0, v0x5f36dfebd150_0;  1 drivers
v0x5f36dfebcf60_0 .net "y", 0 0, L_0x5f36dfebd320;  alias, 1 drivers
    .scope S_0x5f36dfeab240;
T_0 ;
    %vpi_call 2 9 "$dumpfile", "or_gate_results.vcd" {0 0 0};
    %vpi_call 2 10 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5f36dfeab240 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f36dfebd080_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f36dfebd150_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f36dfebd080_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f36dfebd150_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f36dfebd080_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f36dfebd150_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f36dfebd080_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f36dfebd150_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 18 "$display", "OR gate tests complete" {0 0 0};
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
